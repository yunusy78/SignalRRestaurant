@model DtoLayer.DiningTableDtos.CreateDiningTableDto

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="main-panel">
<div class="content">
<div class="container-fluid">
    <h4 class="page-title">Bord</h4>
    <div class="row">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">
                    <div class="card-title">Legg til nytt Bord</div>
                </div>
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data" action="/Admin/AdminDiningTable/Create">
                        @Html.AntiForgeryToken()
                        <div class="form-group">
                            <label>Navn:</label>
                            <input asp-for="TableName" type="text" class="form-control" >
                        </div>
                        <div class="form-group">
                            <label>Kapasitet :</label>
                            <input asp-for="Capacity" type="number" class="form-control" >
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Lagre</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>