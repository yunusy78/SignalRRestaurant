@model DtoLayer.ContactDtos.CreateContactDto

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="main-panel">
<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Kontakt</h4>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <div class="card-title">Legg til ny Kontakt</div>
                    </div>
                    <div class="card-body">
                        <form method="post" enctype="multipart/form-data" action="/Admin/AdminContact/Create">
                            @Html.AntiForgeryToken()
            
                            <div class="form-group">
                                <label>Lokasjon :</label>
                                <input asp-for="Location" class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label>Beskrivelse :</label>
                                <input asp-for="FooterDescription" class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label>E-Post :</label>
                                <input asp-for="Email" type="email" class="form-control" required>
                            </div>
                            <div class="form-group">
                                <label>Telefon :</label>
                                <input asp-for="Phone" class="form-control" required>
                            </div>
                            <div class="text-center">
                                <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Lagre</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
</div>