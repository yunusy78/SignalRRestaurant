@model DtoLayer.SocialMediaDtos.CreateSocialMediaDto

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="main-panel">
<div class="content">
<div class="container-fluid">
    <h4 class="page-title">Produkt</h4>
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <div class="card-title">Legg til ny Sosiale Medie</div>
                </div>
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data" action="/Admin/AdminSocialMedia/Create">
                        @Html.AntiForgeryToken()
            
                        <div class="form-group">
                            <label>Title:</label>
                            <input asp-for="Title" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label>Ikon :</label>
                            <input asp-for="Icon" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label>Url :</label>
                            <input asp-for="Url" class="form-control" required>
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Lagre</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>