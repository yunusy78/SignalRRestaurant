@model DtoLayer.BookingDtos.CreateReservationDto

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="main-panel">
<div class="content">
<div class="container-fluid">
    <h4 class="page-title">Bestilling</h4>
    <div class="row">
        <div class="col-md-6">
            <div class="card">
                <div class="card-header">
                    <div class="card-title">Legg til ny Bestilling</div>
                </div>
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data" action="/Admin/AdminReservation/Create">
                        @Html.AntiForgeryToken()
                        <div class="form-group">
                            <label>Navn:</label>
                            <input asp-for="FullName" type="text" class="form-control" >
                        </div>
                        <div class="form-group">
                            <label>E-Post :</label>
                            <input asp-for="Email" type="email" class="form-control" >
                        </div>
                        <div class="form-group">
                            <label>Telefon :</label>
                            <input asp-for="Phone" type="text" class="form-control" >
                        </div>
                        <div class="form-group">
                            <label>Anntall Person :</label>
                            <input asp-for="PersonCount"  type="number" class="form-control" >
                        </div>
                        <div class="form-group">
                            <label>Bestilling Dato :</label>
                            <input asp-for="CreatedAt"  type="datetime-local" class="form-control" >
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Lagre</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>