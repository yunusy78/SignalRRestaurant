@model DtoLayer.FeatureDtos.ResultFeatureDto

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}
<div class="main-panel">
<div class="content">
<div class="container-fluid">
    <h4 class="page-title">Funksjon</h4>
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <div class="card-title">Rediger Funksjon</div>
                </div>
                <div class="card-body">
                    <form method="post" enctype="multipart/form-data" action="/Admin/AdminFeature/Update">
                        @Html.AntiForgeryToken()
                        <input asp-for="FeatureId" type="hidden" class="form-control" >
                        <div class="form-group">
                            <label>Title :</label>
                            <input asp-for="Title" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label>Beskrivelse :</label>
                            <input asp-for="Description" class="form-control" required>
                        </div>
                        <div class="mb-3">
                            <label>Icon Url</label>
                            <input type="file" id="uploadBox" name="file1" class="form-control" />
                        </div>
                        <div class="text-center">
                            <button type="submit" class="btn btn-primary"><i class="fa fa-save"></i> Lagre</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</div>